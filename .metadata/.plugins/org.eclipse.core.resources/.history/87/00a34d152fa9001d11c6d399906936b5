package TP1;

public class Client {
	
	private int code;
	private String nom;
	private String prenom;
	private Voiture tabv []; //=new Voiture[5]
	private int nbvoitures = 0;
	
	public Client(int code , String nom ,String prenom )
	{
		this.code=code;
		this.nom=nom;
		this.prenom=prenom;
		tabv=new Voiture[5];
	}
	
	public void louerVoitur(Voiture v)
	{
		if (nbvoitures < tabv.length)
		{
			tabv[nbvoitures] = v;
		}
		
	}

}
