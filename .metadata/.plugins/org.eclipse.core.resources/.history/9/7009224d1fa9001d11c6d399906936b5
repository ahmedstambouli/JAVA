package TP1;

public class Voiture 

{

	private String immatriculation =" " ;
	private String marque =" " ;
	private double prix;
	
	public String getImmatriculation ()
	{
		return immatriculation;
	}
	
	public String getMarque ()
	{
		return marque;
	}
	
	public double getPrix()
	{
		return prix;
	}
	
	public void setImmatriculation(String im)
	{
		immatriculation=im;
	}
	
	public void setMarque (String ma)
	{
		marque=ma;
	}
	
	public void setPrix(float p)
	{
		prix=p;
	}
	
	
	public Voiture ()
	{
		immatriculation="";
		marque="";
		prix=0;
	}
	
	public Voiture (String ima , String mar , float pr)
	{
		immatriculation=ima;
		marque=mar;
		prix=pr;
	}
	
	
	public String  toString ()
	{
		return ("imatriculation de voiture : "+immatriculation+" et marque : "+marque+" et le prix :"+prix);
	}
	
	
	public boolean equals (Voiture v) {
		return ((this.immatriculation == v.immatriculation) && (this.marque == v.marque) && (this.prix == v.prix));
		
	}
	
	
	
	
	}
